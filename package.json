{
  "name": "castle-clash",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/castle-clash.git"
  },
  "scripts": {
    "initApp": "meteor npm prune && meteor npm i",
    "prestart": "meteor npm run initApp",
    "predebug": "meteor npm run initApp",
    "start": "meteor run --inspect-brk --settings settings.json",
    "debug": "meteor debug --inspect-brk --settings settings.json",
    "test": "meteor test --driver-package=practicalmeteor:mocha --port=3004"
  },
  "dependencies": {
    "autoprefixer": "7.2.3",
    "babel-runtime": "^6.26.0",
    "bootstrap": "4.0.0-beta.2",
    "formik": "0.10.5",
    "jquery": "3.2.1",
    "meteor-node-stubs": "^0.3.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-popper": "0.7.4",
    "react-router-dom": "4.2.2",
    "reactstrap": "5.0.0-alpha.4",
    "sweetalert": "2.1.0",
    "yup": "0.23.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "ie >= 9",
          "and_chr >= 2.3"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "eslint": "4.13.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.5.1",
    "jshint": "2.9.5"
  }
}
